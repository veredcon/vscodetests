{
	"name": "vscode-ext-sample",
	"displayName": "vscode-ext-sample",
	"description": "Simple example for VS Code",
	"version": "0.0.2",
	"publisher": "SAP",
	"repository": "https://github.wdf.sap.corp/I065595/vscodecontextmenu",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showDialog",
				"title": "Show Save Dialog"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"when": "false",
					"command": "extension.showDialog"
				}
			],
			"explorer/context": [
				{
					"when": "resourceExtname =~ /(xml|json|txt|cds|csn)/",
					"command": "extension.showDialog",
					"group": "MyGroup@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc  -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"@types/vscode": "^1.32.0",
		"vsce": "^1.61.0"
	}
}
